{
  "name": "workerman/webman",
  "type": "project",
  "keywords": [
    "high performance",
    "http service"
  ],
  "homepage": "https://www.workerman.net",
  "license": "MIT",
  "description": "High performance HTTP Service Framework.",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "https://www.workerman.net",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "walkor@workerman.net",
    "issues": "https://github.com/walkor/webman/issues",
    "forum": "https://wenda.workerman.net/",
    "wiki": "https://workerman.net/doc/webman",
    "source": "https://github.com/walkor/webman"
  },
  "require": {
    "php": ">=8.1",
    "workerman/webman-framework": "^2.1",
    "monolog/monolog": "^2.0",
    "webman/console": "^2.1",
    "kalnoy/nestedset": "^6.0",
    "webman/database": "^2.1",
    "illuminate/pagination": "^12.20",
    "illuminate/events": "^12.20",
    "symfony/var-dumper": "^7.3",
    "firebase/php-jwt": "^6.11",
    "webman-tech/laravel-validation": "^12.0",
    "webman-tech/laravel-translation": "^12.0",
    "phpoption/phpoption": "^1.9",
    "vlucas/phpdotenv": "^5.6",
    "webman/cors": "^1.0",
    "casbin/webman-permission": "^2.1",
    "aovol/webman-auth": "dev-main",
    "robmorgan/phinx": "^0.16.10",
    "aovol/webman-resource": "dev-main",
    "casbin/database-adapter": "^2.1",
    "webman/redis": "^2.1",
    "webman/event": "^1.0"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app",
      "App\\": "./app",
      "app\\View\\Components\\": "./app/view/components",
      "Aovol\\WebmanAuth\\": "./vendor/aovol/webman-auth/src"
    }
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
